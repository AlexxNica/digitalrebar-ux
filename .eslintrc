{
  "env": {
    "es6": true,
    "browser": true
  },
  "globals": {
    "angular": true,
    "$": true,
    "vis": true
  },
  "ecmaVersion": 6,
  "parser": "esprima",
  "extends": "eslint:recommended",
  "rules": {
    "quotes": ["error", "single"],
    "no-var": "warn",
    "semi": ["error", "always"],
    "indent": ["error", 2],
    "eqeqeq": ["warn", "always"],
    "no-unused-vars": "warn",
    "no-console": "off",
    "no-case-declarations": "off",
    "no-eq-null": "error",
    "no-eval": "error",
    "no-trailing-spaces": "warn",
    "no-useless-return": "warn",
    "max-len": ["error", 80],
    "dot-location": ["error", "property"]
  }
}